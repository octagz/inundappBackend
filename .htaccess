<VirtualHost *:80>
    ServerName hosting.cs.uns.edu.ar
    ServerAlias www.hosting.cs.uns.edu.ar

    DocumentRoot /home/www-users/inundapp/public_html/inundapp/web
    <Directory /home/www-users/inundapp/public_html/inundapp/web>
        AllowOverride All
        Require all granted

        <IfModule mod_rewrite.c>
            Options -MultiViews
            RewriteEngine On
            RewriteCond %{REQUEST_FILENAME} !-f
            RewriteRule ^(.*)$ app.php [QSA,L]
        </IfModule>
    </Directory>
    ErrorLog /var/log/apache2/symfony_error.log
    CustomLog /var/log/apache2/symfony_access.log combined
</VirtualHost>
